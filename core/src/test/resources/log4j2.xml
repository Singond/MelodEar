<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
	<Appenders>
		<Console name="Log" target="SYSTEM_OUT">
			<PatternLayout pattern="[%t] %-24.24c{1} %-5level | %msg%n"/>
		</Console>
	</Appenders>
	<Loggers>
		<Root level="debug">
			<AppenderRef ref="Log"/>
		</Root>
	</Loggers>
</Configuration>
