<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<DialogPane xmlns:fx="http://javafx.com/fxml/1" fx:id="settingsDlg">
	<content>
		<BorderPane fx:id="settingsPane" minWidth="800">
			<left>
				<ListView fx:id="sectionSelect" styleClass="section-select"
						prefWidth="200">
					<BorderPane.margin>
						<Insets right="16"/>
					</BorderPane.margin>
				</ListView>
			</left>
		</BorderPane>
	</content>
	<buttonTypes>
		<ButtonType fx:constant="APPLY"/>
		<ButtonType fx:constant="CANCEL"/>
	</buttonTypes>
</DialogPane>
